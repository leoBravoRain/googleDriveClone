{
    "Version": "2025-08-17",
    "Statement": [
        {
            "Effect": "Allow",           
            "Principal": {"AWS": "*"},   
            "Action": ["s3:GetObject", "s3:ListBucket", "s3:PutObject", "s3:DeleteObject"],  
            "Resource": ["filedrive/*"]
        },
        {
            "Effect": "Deny",
            "Principal": {"AWS": "*"},
            "Action": ["s3:DeleteBucket"],
            "Resource": ["filedrive/*"]
        }
    ]
}